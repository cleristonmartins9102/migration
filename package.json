{
  "name": "scaffolding",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start:production": "node --env-file=.env.production src/main/server.js",
    "start:staging": "node --env-file=.env.staging src/main/server.js",
    "start:dev": "env-cmd -f .env.local ts-node-dev src/main/server.ts",
    "build": "rimraf build && tsc --build && copyfiles package.json build && copyfiles .npmrc build && copyfiles permissions*.* build && copyfiles .env*.* build",
    "install:dep:dev": "npm i --legacy-peer-deps",
    "install:dep:prod": "npm run install:dep:dev -- --omit=dev",
    "test": "jest --runInBand --no-cache --passWithNoTests --coverage false",
    "test:watch": "npm run test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coveral": "npm test -- --coverage",
    "install:common": "npm i @adamsfoodservice/core-models@latest @adamsfoodservice/shared-modules@latest "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.5.0",
    "@types/cookie-session": "^2.0.49",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^7.14.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "copyfiles": "^2.4.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "globals": "^15.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "lint-staged": "^15.2.7",
    "pg-mem": "^2.8.1",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "timekeeper": "^2.3.1",
    "ts-jest": "^29.1.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.14.1"
  },
  "dependencies": {
    "@adamsfoodservice/core-models": "^1.0.30",
    "@adamsfoodservice/shared-modules": "^1.0.49",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "module-alias": "^2.2.3",
    "typeorm": "^0.3.20"
  }
}