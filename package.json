{
  "name": "afsservice-scaffolding",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start:production": "node --env-file=.env.production src/main/server.js",
    "start:staging": "node --env-file=.env.staging src/main/server.js",
    "start:dev": "env-cmd -f .env.local ts-node-dev src/main/server.ts",
    "dev:local": "env-cmd -f .env.local ts-node-dev --watch --respawn --transpile-only node_modules src/index.ts",
    "dev:sandbox": "env-cmd -f .env.sandbox ts-node-dev --watch --respawn --transpile-only node_modules src/index.ts",
    "dev:start:sandbox:docker": "npm run build && docker-compose -f docker-compose-dev.yaml up",
    "dev:stop:sandbox:docker": "docker-compose -f docker-compose.yaml down",
    "build": "rimraf build && tsc --build && copyfiles package.json build && copyfiles .npmrc build && copyfiles permissions*.* build && copyfiles .env*.* build",
    "deploy:docker": "docker build -t afsservice-authentication-service . && docker run -it -p 6001:6001 --rm afsservice-authentication-service",
    "install:dep:dev": "npm i --legacy-peer-deps",
    "install:dep:prod": "npm run install:dep:dev -- --omit=dev",
    "test": "jest --runInBand --no-cache --passWithNoTests --coverage false",
    "test:watch": "npm run test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coveral": "npm test -- --coverage",
    "test:ci": "jest --ci --runInBand --coverage",
    "pre:build": "npm run lint && npm run test:ci",
    "lint": "eslint --fix",
    "install:common": "npm i @adamsfoodservice/core-models@latest @adamsfoodservice/shared-modules@latest "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@faker-js/faker": "^8.4.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/opener": "^1.4.3",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "copyfiles": "^2.4.1",
    "env-cmd": "^10.1.0",
    "eslint": "^9.8.0",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "lint": "^0.8.19",
    "lint-staged": "^15.2.7",
    "open": "^10.1.0",
    "pg-mem": "^2.8.1",
    "prisma": "^5.17.0",
    "supertest": "^7.0.0",
    "timekeeper": "^2.3.1",
    "ts-jest": "^29.1.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.18.0"
  },
  "dependencies": {
    "@adamsfoodservice/core-models": "^1.0.70",
    "@adamsfoodservice/shared-middleware": "^1.0.54",
    "@adamsfoodservice/shared-modules": "^1.0.280",
    "@apollo/server": "^4.10.5",
    "@prisma/client": "^5.17.0",
    "@redis/client": "^1.5.17",
    "@types/cookie-session": "^2.0.49",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "apollo-server-express": "^3.13.0",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "open-cli": "^8.0.0",
    "opener": "^1.5.2",
    "pg": "^8.12.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  },
  "_moduleAliases": {
    "@": "src/."
  }
}
